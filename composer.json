{
    "name": "gewis/gewisweb",
    "description": "GEWIS Website",
    "license": "GPL-3.0-only",
    "keywords": [
        "website",
        "gewis"
    ],
    "homepage": "https://gewis.nl/",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/jszanto/gewisweb-test-data"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-imagick": "^3.5.0",
        "ext-mbstring": "*",
        "ext-memcached": "^3.0.0",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-pdo_pgsql": "*",
        "ext-pgsql": "*",
        "ext-soap": "*",
        "ext-zend-opcache": "*",
        "ext-zip": "^1.12.0",
        "laminas/laminas-authentication": "^2.6.0",
        "laminas/laminas-cache": "^2.11.0",
        "laminas/laminas-cache-storage-adapter-filesystem": "^1.1.0",
        "laminas/laminas-cache-storage-adapter-memcached": "^1.2.0",
        "laminas/laminas-captcha": "^2.10.0",
        "laminas/laminas-cli": "^1.1.0",
        "laminas/laminas-code": "^3.5.0",
        "laminas/laminas-crypt": "^3.4.0",
        "laminas/laminas-dependency-plugin": "^2.1.0",
        "laminas/laminas-escaper": "^2.8.0",
        "laminas/laminas-eventmanager": "^3.3.0",
        "laminas/laminas-form": "^3.0.0",
        "laminas/laminas-http": "^2.14.0",
        "laminas/laminas-hydrator": "^4.2.0",
        "laminas/laminas-i18n": "^2.11.0",
        "laminas/laminas-inputfilter": "^2.12.0",
        "laminas/laminas-json": "^3.2.0",
        "laminas/laminas-log": "^2.13.0",
        "laminas/laminas-mail": "^2.14.0",
        "laminas/laminas-mime": "^2.8.0",
        "laminas/laminas-modulemanager": "^2.10.0",
        "laminas/laminas-mvc": "^3.2.0",
        "laminas/laminas-mvc-i18n": "^1.2.0",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.3.0",
        "laminas/laminas-paginator": "^2.10.0",
        "laminas/laminas-permissions-acl": "^2.8.0",
        "laminas/laminas-router": "^3.4.0",
        "laminas/laminas-serializer": "^2.10.0",
        "laminas/laminas-servicemanager": "^3.6.0",
        "laminas/laminas-session": "^2.11.0",
        "laminas/laminas-soap": "^2.9.0",
        "laminas/laminas-stdlib": "^3.4.0",
        "laminas/laminas-validator": "^2.14.0",
        "laminas/laminas-view": "^2.12.0",
        "doctrine/orm": "^2.9.3",
        "doctrine/doctrine-module": "^4.2.0",
        "doctrine/doctrine-orm-module": "^4.0.0",
        "doctrine/doctrine-laminas-hydrator": "^2.1.0",
        "maglnet/magl-markdown": "^1.8",
        "twbs/bootstrap-sass": "^3.3",
        "firebase/php-jwt": "^5.4.0",
        "monolog/monolog": "^2.3.0",
        "league/glide": "^2.0.0",
        "fortawesome/font-awesome": "^5.15.3",
        "slowprog/composer-copy-file": "^0.3.0",
        "cweagans/composer-patches": "^1.7.1"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "ext-xdebug": "^3.0.0",
        "laminas/laminas-component-installer": "^2.5.0",
        "laminas/laminas-developer-tools": "^2.1.0",
        "laminas/laminas-loader": "^2.7.0",
        "san/san-session-toolbar": "^4.0.0",
        "jszanto/gewisweb-test-data": "@dev",
        "squizlabs/php_codesniffer": "^3.6.0",
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "phpstan/phpstan": "^1.0.0",
        "phpstan/extension-installer": "^1.1",
        "slam/phpstan-laminas-framework": "^1.0.0",
        "phpstan/phpstan-doctrine": "^1.0.0"
    },
    "replace": {
        "laminas/laminas-cache-storage-adapter-apc": "*",
        "laminas/laminas-cache-storage-adapter-apcu": "*",
        "laminas/laminas-cache-storage-adapter-blackhole": "*",
        "laminas/laminas-cache-storage-adapter-dba": "*",
        "laminas/laminas-cache-storage-adapter-ext-mongodb": "*",
        "laminas/laminas-cache-storage-adapter-memcache": "*",
        "laminas/laminas-cache-adapter-storage-memory": "*",
        "laminas/laminas-cache-storage-adapter-mongodb": "*",
        "laminas/laminas-cache-storage-adapter-redis": "*",
        "laminas/laminas-cache-storage-adapter-session": "*",
        "laminas/laminas-cache-storage-adapter-wincache": "*",
        "laminas/laminas-cache-storage-adapter-xcache": "*",
        "laminas/laminas-cache-storage-adapter-zend-server": "*"
    },
    "config": {
        "preferred-install": {
            "doctrine/orm": "source",
            "*": "dist"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ],
        "post-update-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ]
    },
    "extra": {
        "copy-file": {
            "vendor/fortawesome/font-awesome/scss/": "public/scss/fontawesome/",
            "vendor/fortawesome/font-awesome/webfonts/": "public/fonts/fontawesome/"
        },
        "patches": {
            "doctrine/orm": {
                "Fix issues with SubDecisions.": "https://raw.githubusercontent.com/GEWIS/orm/2.10.x/1-to-1-multiple-join-columns.patch"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Activity\\": "module/Activity/src",
            "Application\\": "module/Application/src",
            "Company\\": "module/Company/src",
            "Decision\\": "module/Decision/src",
            "Education\\": "module/Education/src",
            "Frontpage\\": "module/Frontpage/src",
            "Photo\\": "module/Photo/src",
            "User\\": "module/User/src"
        }
    }
}
